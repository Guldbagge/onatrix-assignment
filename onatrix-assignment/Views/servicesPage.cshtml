@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@inject Umbraco.Cms.Web.Common.UmbracoHelper UmbracoHelper

@{
	Layout = "mainLayout.cshtml";
    ViewBag.Title = Model?.Value("pageTitle") ?? null;
    var servicesBannerBackroundImage = Model?.Value<IPublishedContent>("servicesBannerBackroundImage")?.Url() ?? "";
}

<section id="contact" class="banner bg-gray">
    <div class="background"> 
        <img src="@servicesBannerBackroundImage" alt="Backround image">
        <div class="content">
            <h2>@Model?.Value("servicesBannerHeadline")</h2>
            <div class="links">
                <a class="home" href="@Model?.Value("servicesBannerLink1")">@Model?.Value("servicesBannerLinkText1")</a>
                    <div class="chevron"><i class="fa-regular fa-chevron-right"></i></div>
                <a class="activ" href="@Model?.Value("servicesBannerLink2")">@Model?.Value("servicesBannerLinkText2")</a>
            </div>
        </div>
    </div>
</section>


@await Html.PartialAsync("partials/static/services-section.cshtml")

<div class="pagnaition">
    <a href="#" class="prev">Prev</a>
    <a href="#" class="one">1</a>
    <a href="#" class="two">2</a>
    <a href="#" class="next">Next</a>
</div>


@await Html.PartialAsync("partials/static/next-project.cshtml")

